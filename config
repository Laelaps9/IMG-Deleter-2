# Unmark for deletion
unmark: j

# Mark for deletion
mark: k

# Movement
previous: h
next: l

# Complete
stop: s

# Image Position
# Shows where the image will be displayed
position: 1366x0

# Comparison threshold (between -1 and 1).
# A value of -1 means that the images are completely different
# A value of 1 means that the images are perfectly similar
# Images with a value above the threshold will be considered duplicates
comp_threshold: 0.9
